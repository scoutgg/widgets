{
  "name": "@scoutgg/widgets",
  "version": "2.2.3",
  "description": "Small webcomponent library for simple apps",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./esm": "./esm/index.js",
    "./vdom": {
      "import": "./esm/renderers/vdom.js",
      "require": "./cjs/renderers/vdom.js"
    },
    "./hyper": {
      "import": "./esm/renderers/hyper.js",
      "require": "./cjs/renderers/hyper.js"
    },
    "./react": {
      "import": "./esm/renderers/react.js",
      "require": "./cjs/renderers/react.js"
    },
    "./queue": {
      "import": "./esm/renderers/queue.js",
      "require": "./cjs/renderers/queue.js"
    }
  },
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "start": "npx local-web-server --static.index demo/index.html --open",
    "readme": "projectz compile",
    "build": "npm run cjs | npm run readme",
    "cjs": "ascjs esm cjs"
  },
  "keywords": [],
  "author": "Scout Gaming Group <oystein.olsen@scoutgaminggroup.com>",
  "license": "ISC",
  "devDependencies": {
    "ascjs": "^5.0.1",
    "projectz": "^2.18.0"
  },
  "maintainers": [],
  "contributors": [
    "Øystein Ø. Olsen (https://github.com/FireNeslo)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scoutgg/widgets.git"
  },
  "bugs": {
    "url": "https://github.com/scoutgg/widgets/issues"
  },
  "homepage": "https://github.com/scoutgg/widgets#readme",
  "dependencies": {
    "rollup": "^2.39.0"
  }
}
